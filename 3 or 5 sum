#include <iostream>
using namespace std;

int main() {
    // Start typing your code here...
   int m;
   cin>>m;
   while(m>0)
   {
    long long int t,i,sum=0,sum1=0,sum2=0,sum3=0;
    cin>>t;
    t=t-1;
    
    i=t/3;
    sum=sum+(i*(6+(i-1)*3))/2;
    
    i=t/5;
    sum=sum+(i*(10+(i-1)*5))/2;
    
    i=t/15;
    
    sum=sum-(i*(30+(i-1)*15))/2;
    
   cout<<sum<<endl;
    m--;
   
   }
    
    return 0;
}
